# Frontend Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Application Configuration
VITE_APP_TITLE=Molecular Analysis Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Advanced molecular docking and analysis platform

# Features Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK_DATA=false

# Authentication Configuration
VITE_JWT_ISSUER=mad.local
VITE_JWT_AUDIENCE=mad

# 3Dmol.js Configuration
VITE_ENABLE_3DMOL=true
VITE_3DMOL_CDN=https://cdnjs.cloudflare.com/ajax/libs/3dmol/2.0.4/3Dmol-min.js

# Performance Configuration
VITE_CHUNK_SIZE_WARNING_LIMIT=1000
VITE_ENABLE_PWA=false

# Development Configuration
VITE_DEV_SERVER_PORT=3000
VITE_DEV_SERVER_HOST=localhost
